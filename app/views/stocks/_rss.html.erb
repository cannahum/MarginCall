<tbody>
  <strong><%= @stock.ticker %> In The News </strong> 
  </br>
  <% prev_title = ""%>
  <% @rss.items.each do |item| %>
    <% if item.title != prev_title%>
      <ul>
      
        <li><%= link_to "#{item.title}", item.link %></li>
        
      </ul>
      <% prev_title = item.title%>
    <%end%>
  <% end %>
</tbody>